package soldado;

public class Main {

	public static void main(String[] args) {
		Soldado s = new Soldado();
		
		System.out.println();
		System.out.println(s);
		System.out.println(s.recibioDisparo());
		
		System.out.println();
		System.out.println(s);
		System.out.println(s.recibioCuracion());
		
		System.out.println();
		System.out.println(s);
		System.out.println(s.recibioDisparo());
		
		System.out.println();
		System.out.println(s);
		System.out.println(s.recibioDisparo());		
		
		System.out.println();
		System.out.println(s);
		System.out.println(s.recibioCuracion());
		
		System.out.println();
		System.out.println(s);

	}

}
